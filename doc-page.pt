        <div id="message" style="widht:100%;">
            <div tal:condition="view.message"
                 tal:attributes="class string:center c alert alert-${view.level} alert-dismissible"
                 role="alert"
                 style="width:70%; margin-top:1em;"
            >
                <span tal:content="structure str(view.message)">Message</span>
                <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true" class="ion ion-close"></span></button>
            </div>
        </div>
        <div id="controls">
            <!-- http://ionicons.com/ -->
            <button class="btn btn-default"
                    data-toggle="button"
                    aria-pressed="false"
                    autocomplete="off"
                    id="panel-toggle-button"
            ><i class="ion ion-android-menu"></i></button>
            <!-- http://fontawesome.io/icons/ -->
            <div class="hidden" id="controls-buttons">
                <div class="btn-group"
                     style="margin-bottom:10pt;"
                     data-class="s100"
                     id="ctrl-btn-text-size"
                >
                    <button class="btn btn-primary"
                            id="ctrl-btn-12pt"
                            data-toggle="button"
                            aria-pressed="false"
                            data-class="s12pt"
                    >12pt</button>
                    <button class="btn btn-primary"
                            id="ctrl-btn-14pt"
                            data-toggle="button"
                            aria-pressed="false"
                            data-class="s14pt"
                    >14pt</button>
                    <button class="btn btn-primary"
                            id="ctrl-btn50"
                            data-toggle="button"
                            aria-pressed="false"
                            data-class="s50"
                    >50%</button>
                    <button class="btn btn-primary active"
                            id="ctrl-btn-100"
                            data-toggle="button"
                            aria-pressed="true"
                            data-class="s100"
                    >100%</button>
                </div><br/>
                <div class="btn-group"
                     style="margin-bottom:10pt;"
                >
                    <button class="btn btn-default"
                            id="cmd-save-button"
                    >
                        <i class="ion ion-archive"></i>
                    </button>
                    <button class="btn btn-success"
                            id="cmd-commit-button"
                    >
                        <i class="ion ion-checkmark-round"></i>
                    </button>
                    <button class="btn btn-primary"
                            id="cmd-branch-button"
                    >
                        <i class="ion ion-fork-repo"></i>
                    </button>
                    <button class="btn btn-default"
                            id="cmd-merge-button"
                    >
                        <i class="ion ion-merge"></i>
                    </button>
                </div>
                <div class="btn-group right"
